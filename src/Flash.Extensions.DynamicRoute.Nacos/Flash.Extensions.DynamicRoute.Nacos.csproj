<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RootNamespace>Flash.Extensions.DynamicRoute.Nacos</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\SharedAssemblyInfo.cs" Link="Properties\SharedAssemblyInfo.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="nacos-sdk-csharp" Version="1.3.2" />
    <PackageReference Include="nacos-sdk-csharp.AspNetCore" Version="1.3.2" />
    <PackageReference Include="nacos-sdk-csharp.Extensions.Configuration" Version="1.3.2" />
    <PackageReference Include="nacos-sdk-csharp.IniParser" Version="1.3.2" />
    <PackageReference Include="nacos-sdk-csharp.YamlParser" Version="1.3.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Flash.Core\Flash.Core.csproj" />
    <ProjectReference Include="..\Flash.DynamicRoute\Flash.DynamicRoute.csproj" />
  </ItemGroup>

</Project>
